@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ctx: <http://contextdb.jl/kg#> .

ctx:file_1 rdf:type ctx:File .
ctx:file_1 ctx:name "<unknown>" .
ctx:file_1 ctx:start_line "1" .
ctx:file_1 ctx:end_line "3" .

ctx:ref_7 rdf:type ctx:Reference .
ctx:ref_7 ctx:name "mod" .
ctx:ref_7 ctx:start_line "0" .
ctx:ref_7 ctx:end_line "0" .

ctx:func_2 rdf:type ctx:Function .
ctx:func_2 ctx:name "foo" .
ctx:func_2 ctx:signature "foo(x::Int32, n::Int32)" .
ctx:func_2 ctx:start_line "0" .
ctx:func_2 ctx:end_line "0" .

ctx:param_4 rdf:type ctx:Parameter .
ctx:param_4 ctx:name "n" .
ctx:param_4 ctx:type "Int32" .
ctx:param_4 ctx:start_line "0" .
ctx:param_4 ctx:end_line "0" .

ctx:call_6 rdf:type ctx:CallSite .
ctx:call_6 ctx:signature "mod(x, n)" .
ctx:call_6 ctx:start_line "0" .
ctx:call_6 ctx:end_line "0" .

ctx:param_3 rdf:type ctx:Parameter .
ctx:param_3 ctx:name "x" .
