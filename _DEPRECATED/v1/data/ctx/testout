ctx:ret_5 ctx:start_line "0" .
ctx:ret_5 ctx:end_line "0" .

ctx:file_1 ctx:defines ctx:func_2 .
ctx:func_2 ctx:has_param ctx:param_3 .
ctx:func_2 ctx:has_param ctx:param_4 .
ctx:func_2 ctx:contains ctx:ret_5 .
ctx:ret_5 ctx:contains ctx:call_6 .
ctx:call_6 ctx:calls ctx:ref_7 .
ctx:ret_5 ctx:returns ctx:call_6 .
